		<div id="sidebar" class="col-1-3">
			<div class="wrap-sidebar">
				<!---- Start Widget ---->
				<div class="widget wid-about">
					<div class="wid-header">
						<h5><%=commonSlideData.about.title %></h5>
					</div>
					<div class="wid-content">
						<img src="<%=commonSlideData.about.image.path %>" alt="<%=commonSlideData.about.image.alterText %>"/>
						<p><%=commonSlideData.about.description %></p>
					</div>
				</div>
				<!---- Start Widget ---->
				<div class="widget wid-post">
					<div class="wid-header">
						<h5><%=commonSlideData.latestPosts.title %></h5>
					</div>
					<div class="wid-content">
						<% commonSlideData.latestPosts.images.forEach((data)=>{ %>
						<div class="post">
							<a href="<%= data.image.href%>"><img src="<%= data.image.path%>" alt="<%= data.image.alterText%>"/></a>
							<div class="wrapper">
							  <h5><a href="#"><%= data.title%></a></h5>
							   <span><%= data.price%></span>
							</div>
						</div>
						<% }) %>
						
					</div>
				</div>
				<!---- Start Widget ---->
				<div class="widget wid-tag">
					<div class="wid-header">
						
						<h5><%= commonSlideData.tags.title %></h5>
					</div>
					<div class="wid-content">
						<% commonSlideData.tags.tagKeywords.forEach((tag)=>{ %>
						<a href="<%= tag.tagName.href %>"> <%= tag.tagName.keyword %> ,</a>
						<% }) %>
					</div>
				</div>
				<!---- Start Widget ---->
				<div class="widget wid-gallery">
					<div class="wid-header">
						<h5><%= commonSlideData.galleyPic.title %></h5>
					</div>
					<div class="wid-content">
                     <% commonSlideData.galleyPic.images.forEach((data)=>{ %>
						<a href="<%= data.image.href%>"><img src="<%= data.image.path %>" alt="<%= data.image.alterText %>"></a>
						<% }) %>
					</div>
				</div>
			</div>
		</div>
	</div>
